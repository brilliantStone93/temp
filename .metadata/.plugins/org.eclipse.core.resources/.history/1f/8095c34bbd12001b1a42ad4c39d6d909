package day1020;

import javax.swing.JFrame;

public class ExTryNotepad extends JFrame{

	public ExTryNotepad() {
		super("간의 메모장");
		
		
		
		
		setVisible(true);
		setBounds(100, 100, 500, 500);
		setDefaultCloseOperation(EXIT_ON_CLOSE);
		
		
	}
	
	
	public static void main(String[] args) {
		new ExTryNotepad();
		
	}//main
}//
